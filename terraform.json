{
    "extends": [
      "config:base"
    ],
    "semanticCommits": "enabled",
    "packageRules": [{
      "matchDatasources": [
        "terraform-module",
        "terraform-provider"
      ],
      "postUpgradeTasks": {
        "commands": [
          "/tf-docs/tf-docs.sh"
        ],
        "executionMode": "branch",
        "fileFilters": [
          "**/*.tf",
          "README.md"
        ]
      }
    }]
}